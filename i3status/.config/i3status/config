order += "read_file GITHUB"
order += "disk /home"
order += "read_file UPTIME"
order += "read_file I3AGENDA"
order += "tztime local"

general {
	color_good = "#BAFB00"
	color_bad = "#B7211F"
	markup = pango
}

disk "/home" {
    format = "%free"
}
tztime local {
	format = "<span color='#FFE999'>%m-%d</span> <span color='#FFC100'>%H:%M:%S</span>"
}
memory {
	format = "%used/%total"
}
volume master {
    format = "♪: %volume"
    format_muted = "♪: muted (%volume)"
    device = "default"
    mixer = "Master"
    mixer_idx = 0
}
read_file UPTIME {
	color_good = "#ADB8B8"
	format = "%content"
	path = "~/Scripts/outputs/i3status-uptime-output"
}
read_file I3AGENDA{
	color_good = "#FFFFFF"
	format = "%content"
	path = "~/Scripts/outputs/i3status-gcal-output"
}
read_file GITHUB{
	format = "%content"
	path = "~/Scripts/outputs/i3status-github-contributions-output"
}
